{
  "name": "@sapiently/design-tokens",
  "version": "0.1.0",
  "description": "Design tokens for Sapient Design System",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
